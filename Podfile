  pod 'Firebase/Firestore'
  pod 'FirebaseFirestoreSwift' # Codable support for mapping to firestore
  # add pods for any other desired Firebase products
  # https://firebase.google.com/docs/ios/setup#available-pods

  target 'ToDoFirebaseTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'ToDoFirebaseUITests' do
    # Pods for testing
  end

end
